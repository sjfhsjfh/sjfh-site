{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "allowImportingTsExtensions": false,
    "allowJs": true,
    "noEmit": false,
    "sourceMap": true,
    "strictNullChecks": true,
    "declaration": true,
    "declarationMap": true,
  },
}